<!-- Declare DOCTYPE as HTML -->
<!DOCTYPE html>

<!-- HTML tag with language set to English -->
<html lang="en">

<head>
    <!-- Specify character encoding as UTF-8 for proper text rendering -->
    <meta charset="UTF-8">
    
    <!-- Configure viewport for responsive design
         - width=device-width sets width to device screen width
         - initial-scale=1.0 sets initial zoom level -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- Include Bootstrap 5.3.3 CSS from CDN
         - integrity attribute ensures the file hasn't been tampered with
         - crossorigin attribute handles CORS (Cross-Origin Resource Sharing) -->
    <link 
        href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" 
        rel="stylesheet" 
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" 
        crossorigin="anonymous">
    
    <!-- Set page title that appears in browser tab -->
    <title>To Do App</title>
</head>

